var students = ["никита", "илья", "даня", "рома", "ваня", "степа", "дима", "сергей", "миша", "вова", "максим", "влад", "маша", "катя", "соня", "настя", "даша", "лиза", "вика"]

print("Исходный список учеников:")
for student in students {
    print(student)
}

students.removeSubrange(0...6)

print("\nОбновленный список учеников:")
for student in students {
    print(student)
}